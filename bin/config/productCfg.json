{
    "PRODUCT_START": {
        "switch": "!"
    },
    "PRODUCT_BEGIN": {
        "switch": "!"
    },
    "GAME_HOME": {
        "base": [
            {
                "type": "add_more_game",
                "switch": "zs_jump_switch",
                "window": {
                    "align": "left",
                    "alignoffsetx": 30
                }
            }
        ]
    },
    "PRODUCT_HOME_PLAY": {
        "switch": "!"
    },
    "GAME_PLAY": {
        "event": "event_game_play"
    },
    "PRODUCT_PLAY_END": {
        "states": [
            "SHARE",
            "SETTLE"
        ]
    },
    "PRODUCT_PLAY_END.SHARE": {
        "base": [
            {
                "type": "add_share_record",
                "window": {
                    "align": "center",
                    "fit": true,
                    "block": true
                },
                "base": {
                    "event": "event_record_share_reward"
                }
            }
        ]
    },
    "PRODUCT_PLAY_END.SETTLE": {
        "event": "event_init_insert",
        "base": [
            {
                "type": "export_button",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "text": "继续游戏",
                    "event": "event_childnext"
                }
            }
        ]
    },
    "GAME_END": {},
    "PRODUCT_FINISH": {
        "switch": "zs_full_screen_ad",
        "event": [
            "event_load_insert",
            "event_childnext"
        ]
    }
}