{
    "PRODUCT_START": {
        "switch": "!"
    },
    "PRODUCT_BEGIN": {
        "switch": "!"
    },
    "GAME_HOME": {
        "banner": {
            "auto": true,
            "isDelay": true
        },
        "base": [
            {
                "type": "add_btn_deskTopIcon",
                "window": {
                    "align": "right",
                    "scale": [
                        1.5,
                        1.5
                    ]
                }
            },
            {
                "type": "add_btn_moreGame",
                "window": {
                    "align": "left",
                    "scale": [
                        1.5,
                        1.5
                    ]
                }
            }
        ]
    },
    "PRODUCT_HOME_PLAY": {
        "switch": "zs_native_limit",
        "base": {
            "type": "add_screen_native",
            "window": {
                "align": "center",
                "fit": true,
                "block": true
            },
            "base": {
                "closeevent": "event_childnext"
            }
        }
    },
    "GAME_PLAY": {
        "banner": {
            "switch": "zs_game_banner_show_switch"
        }
    },
    "PRODUCT_PLAY_END": {
        "base": [
            {
                "type": "add_insert_native",
                "window": {
                    "align": "center"
                }
            },
            {
                "type": "export_button",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "text": "继续游戏",
                    "event": "event_childnext"
                }
            }
        ]
    },
    "GAME_END": {},
    "PRODUCT_FINISH": {
        "switch": "!"
    }
}