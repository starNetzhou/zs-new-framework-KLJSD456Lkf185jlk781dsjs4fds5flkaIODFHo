{
    "PRODUCT_START": {
        "states": [
            "READY",
            "FULL_1",
            "FULL_2"
        ]
    },
    "PRODUCT_START.READY": {
        "exporter": [
            {
                "type": "export_button",
                "key": "test",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "event": "event_next",
                    "url": [
                        "export/export",
                        "btn-continue-new"
                    ]
                }
            }
        ]
    },
    "PRODUCT_START.FULL_1": {
        "switch": [
            "zs_full_screen2_jump",
            "zs_auto_full_screen_jump_switch"
        ],
        "event": "event_full_2",
        "exporter": [
            {
                "type": "export_button",
                "key": "test",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "switch":"zs_full_screen_button_switch",
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "event": "event_full_continue",
                    "url": [
                        "export/export",
                        "btn-continue-new"
                    ]
                }
            }
        ]
    },
    "PRODUCT_START.FULL_2": {
        "switch": [
            "zs_full_screen1_jump",
            "zs_auto_full_screen_jump_switch"
        ],
        "event": "event_full_1",
        "exporter": [
            {
                "type": "export_button",
                "key": "test",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "switch":"zs_full_screen_button_switch",
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "event": "event_full_continue",
                    "url": [
                        "export/export",
                        "btn-continue-new"
                    ]
                }
            }
        ]
    },
    "PRODUCT_HOME_PLAY": {
        "states": [
            "VIDEO",
            "EGG"
        ]
    },
    "PRODUCT_BEGIN": {
        "switch": "!"
    },
    "PRODUCT_HOME_PLAY.VIDEO": {
        "switch": "zs_start_game_video_switch",
        "event": "event_start_video"
    },
    "PRODUCT_HOME_PLAY.EGG": {
        "check": "check_egg",
        "event": "event_common_egg"
    },
    "GAME_PLAY": {
        "states": [
            "START",
            "READY",
            "PLAY",
            "SETTLE",
            "END"
        ],
        "event": "event_fake_exit"
    },
    "GAME_HOME": {
        "base": [],
        "exporter": [
            {
                "type": "export_list",
                "window": {
                    "align": "topleft",
                    "alignoffsety": 500
                },
                "base": {
                    "item": "export_item_2",
                    "mode": "side",
                    "width": 300,
                    "max": 2,
                    "shaketime": 3000
                }
            },
            {
                "type": "export_list",
                "window": {
                    "align": "topright",
                    "alignoffsety": 500
                },
                "base": {
                    "item": "export_item_2",
                    "mode": "side",
                    "width": 300,
                    "max": 2,
                    "shaketime": 3000
                }
            },
            {
                "type": "export_list",
                "window": {
                    "align": "top",
                    "alignoffsety": 95
                },
                "base": {
                    "item": "export_item_7",
                    "mode": "hlist",
                    "height": 270,
                    "max": 10,
                    "marginleft": 10,
                    "marginright": 10,
                    "background": [
                        "export/export",
                        "icon_background"
                    ]
                }
            },
            {
                "type": "export_side",
                "window": {
                    "align": "left",
                    "alignoffsety": 100,
                    "scale": [
                        0.9,
                        0.9
                    ]
                }
            }
        ]
    },
    "GAME_PLAY.PLAY": {
        "banner": {
            "switch": "zs_game_banner_show_switch"
        },
        "exporter": [
            {
                "type": "export_list",
                "window": {
                    "align": "top",
                    "alignoffsety": 140
                },
                "base": {
                    "item": "export_item_7",
                    "mode": "hlist",
                    "height": 270,
                    "max": 10,
                    "marginleft": 10,
                    "marginright": 10
                }
            }
        ]
    },
    "PRODUCT_PLAY_END": {
        "states": [
            "FULL_1",
            "SETTLE",
            "FULL_2"
        ]
    },
    "PRODUCT_PLAY_END.FULL_1": {
        "switch": [
            "zs_full_screen1_jump"
        ],
        "event": "event_full_1",
        "exporter": [
            {
                "type": "export_button",
                "key": "test",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "switch":"zs_full_screen_button_switch",
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "event": "event_full_continue",
                    "url": [
                        "export/export",
                        "btn-continue-new"
                    ]
                }
            }
        ]
    },
    "PRODUCT_PLAY_END.FULL_2": {
        "switch": [
            "zs_full_screen2_jump"
        ],
        "event": "event_full_2",
        "exporter": [
            {
                "type": "export_button",
                "key": "test",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "switch":"zs_full_screen_button_switch",
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "event": "event_full_continue",
                    "url": [
                        "export/export",
                        "btn-continue-new"
                    ]
                }
            }
        ]
    },
    "PRODUCT_PLAY_END.SETTLE": {
        "switch": [
            "!zs_skip_settle",
            "zs_version"
        ],
        "banner": {
            "delay": true
        },
        "laterevent": [
            "event_fake_exit",
            "event_fake_msg"
        ],
        "exporter": [
            {
                "type": "export_background",
                "window": {
                    "align": "topleft",
                    "fit": true
                }
            },
            {
                "type": "export_knock",
                "switch": "zs_finish_jump",
                "window": {
                    "align": "center",
                    "alignoffsety": -30
                }
            },
            {
                "type": "export_button",
                "key": "test",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "switch":"zs_button_delay_time",
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "event": "event_childnext",
                    "url": [
                        "export/export",
                        "btn-continue-new"
                    ]
                }
            }
        ]
    }
}