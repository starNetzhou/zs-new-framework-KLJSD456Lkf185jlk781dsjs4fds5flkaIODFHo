{
    "PRODUCT_START": {
        "event": "event_enter_game"
    },
    "PRODUCT_BEGIN": {
        "switch": "!"
    },
    "GAME_HOME": {
        "banner": {
            "auto": true
        },
        "base": [
            {
                "type": "qq_btn_more_game",
                "window": {
                    "align": "left",
                    "alignoffsety": -100
                }
            },
            {
                "type": "qq_btn_invite",
                "window": {
                    "align": "left",
                    "alignoffsety": 100
                }
            }
        ],
        "event": {
            "event_add_block_ad": {
                "num": 5,
                "pos": {
                    "top": 150,
                    "centerX": 0
                }
            }
        },
        "exitevent": "event_clear_block_ad"
    },
    "PRODUCT_HOME_PLAY": {
        "states": [
            "COMMON_EGG",
            "TRY_SKIN"
        ]
    },
    "PRODUCT_HOME_PLAY.COMMON_EGG": {
        "check": {
            "event_check_egg": true
        },
        "base": {
            "type": "qq_common_egg",
            "window": {
                "align": "center",
                "fit": true,
                "block": true
            },
            "base": {
                "awardevent": "event_common_egg",
                "closeevent": "event_childnext"
            }
        }
    },
    "PRODUCT_HOME_PLAY.TRY_SKIN": {
        "switch": "zs_switch",
        "check": "event_check_skin",
        "base": {
            "type": "qq_try_skin",
            "window": {
                "align": "center",
                "fit": true,
                "block": true
            },
            "base": {
                "closeevent": "event_childnext",
                "videoevent": "event_try_skin"
            }
        }
    },
    "GAME_PLAY": {
        "states": [
            "PLAY",
            "BOX_EGG"
        ]
    },
    "GAME_PLAY.PLAY": {
        "banner": {
            "auto": true,
            "switch": "zs_game_banner_show_switch"
        },
        "event": {
            "event_add_block_ad": {
                "num": 5,
                "pos": {
                    "top": 150,
                    "centerX": 0
                }
            }
        },
        "exitevent": "event_clear_block_ad"
    },
    "GAME_PLAY.BOX_EGG": {
        "check": {
            "event_check_egg": false
        },
        "base": {
            "type": "qq_box_egg",
            "window": {
                "align": "center",
                "fit": true,
                "block": true
            },
            "base": {
                "closeevent": "event_childnext",
                "awardevent": "event_box_egg"
            }
        }
    },
    "PRODUCT_PLAY_END": {
        "states": [
            "SETTLE"
        ]
    },
    "PRODUCT_PLAY_END.SETTLE": {
        "banner": {
            "auto": true
        },
        "event": {
            "event_add_block_ad": {
                "num": 5,
                "pos": {
                    "centerY": -120,
                    "centerX": 0
                }
            }
        },
        "exitevent": "event_clear_block_ad"
    },
    "PRODUCT_FINISH": {
        "switch": "!"
    }
}