{
    "PRODUCT_START": {
        "base": {
            "type": "add_insert_native",
            "window": {
                "align": "center"
            },
            "base": {
                "closesize": 40,
                "closetouchsize": 30,
                "confirmtext": "点击跳过",
                "confirmswitch": "zs_native_click_switch",
                "closeevent": "event_childnext"
            }
        }
    },
    "PRODUCT_BEGIN": {
        "switch": "!"
    },
    "GAME_HOME": {
        "banner": {
            "auto": true,
            "isDelay": true
        },
        "laterevent": "event_home_banner",
        "exitevent": "event_hide_screen_native",
        "base": [
            {
                "type": "add_btn_deskTopIcon",
                "window": {
                    "align": "right",
                    "scale": [
                        1.5,
                        1.5
                    ]
                }
            }
        ]
    },
    "PRODUCT_HOME_PLAY": {
        "switch": "!"
    },
    "GAME_PLAY": {},
    "PRODUCT_PLAY_END": {
        "event": "event_show_screen_native",
        "exitevent": "event_hide_screen_native",
        "base": [
            {
                "type": "export_button",
                "window": {
                    "align": "bottom",
                    "alignoffsety": -100
                },
                "base": {
                    "width": 349,
                    "height": 160,
                    "offsety": -200,
                    "text": "继续游戏",
                    "event": "event_childnext"
                }
            }
        ]
    },
    "GAME_END": {},
    "PRODUCT_FINISH": {
        "switch": "zs_native_limit",
        "event": [
            "event_show_screen_native",
            "event_hide_banner",
            "event_childnext"
        ]
    }
}